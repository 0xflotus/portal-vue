{
  "name": "vue-portal",
  "version": "0.1.0",
  "description": "a Vue component to render elements  outside of a component's template, elsewhere in the DOM",
  "main": "src/index.js",
  "author": "Thorsten <t.luenborg@googlemail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production node_modules/.bin/rollup -c build/rollup.conf.prod.js && uglifyjs dist/vue-portal.js -c -m > dist/vue-portal.min.js",
    "dev": "NODE_ENV=development webpack-dev-server --watch --config build/webpack.conf.js --inline --hot",
    "docs:dev": "NODE_ENV=development webpack-dev-server --watch --config build/webpack.conf.docs.js --inline --hot",
    "test:unit": "NODE_ENV=testing webpack-dev-server --watch --config build/webpack.test.conf.js --inline --hot"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "browser-env": "^2.0.19",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "cssnext": "^1.8.4",
    "flexboxgrid": "^6.3.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "inject-loader": "2.0.1",
    "mocha": "^3.2.0",
    "mocha-css": "^1.0.1",
    "mocha-loader": "^1.1.0",
    "normalize.css": "^5.0.0",
    "parse5": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "rollup": "^0.38.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-vue": "^2.2.14",
    "sinon": "^1.17.7",
    "tachyons": "^4.6.2",
    "testcafe": "^0.11.1",
    "testdouble": "^1.10.2",
    "testdouble-chai": "^0.5.0",
    "vue": "^2.1.7",
    "vue-loader": "^10.3.0",
    "vue-template-compiler": "^2.1.7",
    "webpack": "2.2.0-rc.2",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.4.0"
  },
  "dependencies": {
    "rollup-plugin-postcss": "^0.2.0",
    "scroll": "^2.0.0",
